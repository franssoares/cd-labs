-- Bibliotecas
LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL;

-- Entidade
ENTITY divide_freq IS
	PORT(
		i0, i1 : IN STD_LOGIC;
		sel : IN STD_LOGIC;
		o : OUT STD_LOGIC
	);
END ENTITY divide_freq;

-- Arquitetura
ARCHITECTURE behav OF divide_freq IS

-- Comportamento
BEGIN

	o <= (i0 AND NOT(sel)) OR (i1 AND sel);

END ARCHITECTURE behav;
